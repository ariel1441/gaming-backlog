{
  "name": "game-backlog-app",
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "node": ">=20 <21"
  },
"scripts": {
    "start": "concurrently -n \"BACK,FRONT\" -c \"cyan,magenta\" \"npm:dev:back\" \"npm:dev:front\"",
    "dev:back": "cross-env NODE_ENV=development nodemon --signal SIGTERM backend/index.js",
    "dev:front": "cross-env NODE_ENV=development vite",
    "serve": "cross-env NODE_ENV=production node backend/index.js",
    "build": "vite build",
    "preview": "cross-env NODE_ENV=production vite preview --port 5173",
    "lint": "eslint .",
    "test": "vitest run --passWithNoTests",
    "check": "npm run lint && npm test"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@heroicons/react": "^2.2.0",
    "axios": "^1.9.0",
    "bcrypt": "^6.0.0",
    "celebrate": "^15.0.3",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "csv-parse": "^6.1.0",
    "csv-stringify": "^6.6.0",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "express-rate-limit": "^8.0.1",
    "helmet": "^8.1.0",
    "howlongtobeat": "^1.8.0",
    "isomorphic-dompurify": "^2.26.0",
    "joi": "^18.0.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.539.0",
    "pg": "^8.16.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.8.0",
    "react-select": "^5.10.1",
    "recharts": "^3.1.2",
    "string-similarity": "^4.0.4",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.4.1",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.0",
    "cross-env": "^10.0.0",
    "nodemon": "^3.0.3",
    "postcss": "^8.5.3",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.17",
    "vite": "^6.3.5"
  }
}
